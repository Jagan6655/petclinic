pipeline {
  agent any
  options {
    buildDiscarder(logRotator(numToKeepStr: '50'))
    timeout(time: 1, unit: 'HOURS')
    timestamps()
  }

  stages {
    stage('checkout') {
      steps {
        git 'https://github.com/Jagan6655/simple-java-maven-app.git'
      }
    }

    stage('compile') {
      steps {
        echo "dummy compilation"
      }
    }

    stage('test') {
      steps {
        echo "sample SCM pipeline is successful"
      }
    }
  }
}
