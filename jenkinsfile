pipeline {
  agent any


  stages {
    stage('code checkout') {
      steps {
        git 'https://github.com/Jagan6655/simple-java-maven-app.git'
      }
    }

    stage('compile') {
      steps {
        echo "dummy compilation"
      }
    }

    stage('test') {
      steps {
        echo "sample SCM pipeline is successful"
      }
    }
  }
}
